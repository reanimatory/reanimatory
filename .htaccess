DirectoryIndex index.html
AddDefaultCharset UTF-8

RewriteCond %{QUERY_STRING} ^IBLOCK_ID=1&SECTION_ID=111$ [NC]
RewriteRule ^dir/index\.php$ /new/sef/? [R=301,L]

RewriteCond %{SERVER_PORT} ^80$ [OR]
RewriteCond %{HTTP} =on
RewriteRule ^(.*)$ https://reanimatory.cf/$1 [R=301,L]

RewriteCond %{HTTP_HOST} ^www.reanimatory\.cf$ [NC]
RewriteRule ^(.*)$ http://reanimatory.cf/$1 [R=301,L]




